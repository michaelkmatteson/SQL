CREATE TABLE mikes_bikes (barcode INTEGER PRIMARY KEY, inventory INTEGER, name TEXT, cost INTEGER);

INSERT INTO mikes_bikes VALUES 
(1, 2, "tandem", 200)
(2, 4, "mountain", 300)
(3, 6,  "beach", 150)
(4, 2,  "street", 200)
(5, 1, "unicycle", 50)
;

SELECT * FROM mikes_bikes ORDER BY cost;
SELECT SUM(INVENTORY) FROM mikes_bikes;
SELECT * FROM mikes_bikes WHERE inventory <=3 ORDER BY inventory asc;
